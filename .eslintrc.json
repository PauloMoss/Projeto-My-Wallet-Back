{
    "env": {
        "node": true,
        "es2021": true
    },
    "extends": ["eslint:recommended", "plugin:jest/recommended"],
    
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
    },
    "overrides": [
        {
          "files": [
            "**/*.spec.js",
            "**/*.spec.jsx"
            ],
          "env": {
            "jest": true
          }
        }
    ],
    "plugins": ["jest"]
}